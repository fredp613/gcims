<%= simple_form_for(@corporation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :level %>
    <%= f.input :businessnumber %>
    <%= f.input :client_id, as: :hidden, :input_html=>{value: @client}  %>
  </div>

  <div class="form-actions">
  	<%= f.button :submit, 'back', :name=>'back_button' %>
    <%= f.button :submit %>
  </div>
<% end %>
