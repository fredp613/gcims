<%= simple_form_for(@applicationcustomtemplate) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :application %>
    <%= f.association :customtemplate %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
