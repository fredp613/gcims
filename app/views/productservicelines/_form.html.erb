
		<%= simple_form_for @productserviceline,:remote=>true, :html => { :class => 'form-horizontal' } do |f| %>
		  <fieldset>
		  	<legend>Product Service Line</legend>
		  <%= f.input :psl_name, :label => "Product Service line (EN)" %>
		  
		 </fieldset>

		 <% if @productserviceline.new_record? %>			 
		  	<fieldset>
		  		<legend>Sub Service Line</legend>
		  		<%= f.input :ssl_name, :label => "Sub Service Line (EN)" %>
		  	</fieldset>				  
		  		<fieldset>
		  		<legend>Summary commitment</legend>
		  		<%= f.input :sc_name, :label => "Summary Commitment (EN)" %>
		  	</fieldset>				  		
		  		<fieldset>
		  		<legend>Commitment Item</legend>
		  		<%= f.input :ci_name, :label => "Commitment Item (EN)" %>
		  	</fieldset>
				  	
		  <% end %>

		 <fieldset>
		    <legend>Fiscal Years</legend>

		       <%= date_picker :startdate, f,  @productserviceline.startdate, "Start Date", @productserviceline.errors %>
  				<%= date_picker :enddate, f,  @productserviceline.enddate, "End Date", @productserviceline.errors %>

		  </fieldset>

		  
		  <div class="form-actions">
		    <%= f.button :submit, :class => 'btn-primary' %>
		    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
		                commitmentitems_path, :class => 'btn' %>
		  </div>

	<% end %>


<script type="text/javascript">

$('.datepicker').datepicker({
  autoclose: 'true',  
  format: 'yyyy-mm-dd'
});


</script>		
