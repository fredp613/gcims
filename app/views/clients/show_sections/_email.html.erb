<% @client.emails.each do |email| %>
       <%= email.email %>
       <%= email.type %>
       <%= link_to '', edit_client_email_path(@client, email), 
       :class=>'edit_main_info icon icon-pencil black',
       'data-attribute'=>'email' %>
       <%= link_to '', email, method: :delete, data: { confirm: 'Are you sure?' },
        :class=>'icon icon-remove-sign red'%><br>
     <% end %>