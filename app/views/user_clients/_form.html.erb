<%= simple_form_for(@user_client) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id, collection: User.all, label_method: :full_name, value_method: :id %>
    <%= f.input :client_id, collection: Client.all, label_method: :adj_name, value_method: :id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
