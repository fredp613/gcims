<%= simple_form_for(@charity) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :registrationnumber %>
    <%= f.input :registrationdate, as: :string,
      :input_html=>{ :class=>'datepicker' } %>
    <%= f.input :client_id, as: :hidden, :input_html=>{value: @client} %>
  </div>

  <div class="form-actions">
  	<%= f.button :submit, 'back', :name=>'back_button' %>
    <%= f.button :submit %>
  </div>
<% end %>
