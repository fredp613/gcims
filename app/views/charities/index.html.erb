<h1>Listing charities</h1>

<table>
  <tr>
    <th>Registrationnumber</th>
    <th>Registrationdate</th>
    <th>Client</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @charities.each do |charity| %>
  <tr>
    <td><%= charity.registrationnumber %></td>
    <td><%= charity.registrationdate %></td>
    <td><%= charity.client_id %></td>
    <td><%= link_to 'Show', charity %></td>
    <td><%= link_to 'Edit', edit_charity_path(charity) %></td>
    <td><%= link_to 'Destroy', charity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Charity', new_charity_path %>
