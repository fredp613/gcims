<%- model_class = Summarycommitment -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:sc_name) %>:</strong></dt>
  <dd><%= @summarycommitment.sc_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:active) %>:</strong></dt>
  <dd><%= @summarycommitment.active %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @summarycommitment.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:subserviceline_id) %>:</strong></dt>
  <dd><%= @summarycommitment.subserviceline_id %></dd>

  <dt><strong>Fiscal Years:</strong></dt>
  <% @summarycommitment.fiscalyears.each do |fy| %>
  <dd><%= fy.fy %></dd>
  <% end %>
  
</dl>

<%= link_to t('.show', :default => t("helpers.links.edit")),
              subserviceline_path(@summarycommitment.subserviceline), :class => 'btn' %>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              summarycommitments_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_summarycommitment_path(@summarycommitment), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              summarycommitment_path(@summarycommitment),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
