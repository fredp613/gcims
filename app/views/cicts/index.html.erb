<h1>Listing cicts</h1>

<table>
  <thead>
    <tr>
      <th>Commitmentitem</th>
      <th>Customtemplate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cicts.each do |cict| %>
      <tr>
        <td><%= cict.commitmentitem.ci_name %></td>
        <td><%= cict.customtemplate.name %></td>
        <td><%= link_to 'Show', cict %></td>
        <td><%= link_to 'Edit', edit_cict_path(cict) %></td>
        <td><%= link_to 'Destroy', cict, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cict', new_cict_path %>
