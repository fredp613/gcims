<%- model_class = Fincode -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:productserviceline_id) %>:</strong></dt>
  <dd><%= @fincode.productserviceline_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:subserviceline_id) %>:</strong></dt>
  <dd><%= @fincode.subserviceline_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:summarycommitment_id) %>:</strong></dt>
  <dd><%= @fincode.summarycommitment_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:commitmentitem_id) %>:</strong></dt>
  <dd><%= @fincode.commitmentitem_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              fincodes_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_fincode_path(@fincode), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              fincode_path(@fincode),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
