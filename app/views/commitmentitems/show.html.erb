<%- model_class = Commitmentitem -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ci_name) %>:</strong></dt>
  <dd><%= @commitmentitem.ci_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:active) %>:</strong></dt>
  <dd><%= @commitmentitem.active %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @commitmentitem.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:summarycommitment_id) %>:</strong></dt>
  <dd><%= @commitmentitem.summarycommitment_id %></dd>

     <dt><strong>Fiscal Years:</strong></dt>
  <% @commitmentitem.fiscalyears.each do |fy| %>
  <dd><%= fy.fy %></dd>
  <% end %>

</dl>

  <%= link_to t('.show', :default => t("helpers.links.edit")),
              summarycommitment_path(@commitmentitem.summarycommitment), :class => 'btn' %>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              commitmentitems_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_commitmentitem_path(@commitmentitem), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              commitmentitem_path(@commitmentitem),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
