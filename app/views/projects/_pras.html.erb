<fieldset>
 	<legend>Financial Coding</legend>
 	<%= f.simple_fields_for :applications do |application| %>
 		<%= application.input :productserviceline_id, label: 'Product Service Line', collection: Productserviceline.all, :label_method => lambda { |psl| "#{psl.psl_name}" }  %>
 		<%= application.input :subserviceline_id, label: 'Sub Service Line', collection: Subserviceline.all, :label_method => lambda { |ssl| "#{ssl.ssl_name}" }  %>
 		<%= application.input :summarycommitment_id, label: 'Summary Commitment', collection: Summarycommitment.all, :label_method => lambda { |sc| "#{sc.sc_name}" }  %>
 		<%= application.input :commitmentitem_id, label: 'Commitment Item', collection: Commitmentitem.all, :label_method => lambda { |ci| "#{ci.ci_name}" }  %>
 	<% end %>

</fieldset>


