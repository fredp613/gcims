
<%= simple_form_for @project, :remote=>true, :html => { :id => "frmOpenProject"}  do |f| %>
 <%= f.error_notification %>

 <input type='hidden' name='ts' value=<%= @stale_form_ts %> />
  <%= f.input :token, as: :hidden, :name=>'token', :input_html=>{value: @token} %>
  <p><h4><%= @client_name %></h4></p>

    <%= f.input :client_id, as: :hidden, :input_html=> {name: 'client_id', value: @client_id}  %><br>
  <div class="form-inputs">

	<%= render 'tombstone', :f=>f %>
	<%= render 'pras', :f=>f %>					
    
  </div>

   <div class="submit">
    <%= f.button :submit, class: 'pull-right btn-success', id: 'openProjectSubmit', style: 'margin-top:-35px;' %>
 </div>	

<script type="text/javascript">

$("#frmOpenProject").formToWizard();

$('.datepicker').datepicker({
  autoclose: 'true',
  format: 'dd-mm-yyyy'
});

</script>

<% end %>



