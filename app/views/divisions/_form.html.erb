<%= simple_form_for(@division) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :name1 %>
    <%= f.input :name2 %>
    <%= f.input :client_id %>
    <%= f.input :location_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
