<h1>Listing divisions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Name1</th>
    <th>Name2</th>
    <th>Client</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @divisions.each do |division| %>
  <tr>
    <td><%= division.name %></td>
    <td><%= division.name1 %></td>
    <td><%= division.name2 %></td>
    <td><%= division.client_id %></td>
    <td><%= division.location_id %></td>
    <td><%= link_to 'Show', division %></td>
    <td><%= link_to 'Edit', edit_division_path(division) %></td>
    <td><%= link_to 'Destroy', division, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Division', new_division_path %>
