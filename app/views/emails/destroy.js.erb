<% if @email.errors.any? %>
 
  
	 console.log('Error');
   $('#'+<%= @email.id %>+'_container').html('<%= j render ('form') %>')


<% else %>
	
  console.log('Deleted');
  
  $('#email_container').html('<%= j render 'clients/email' %>')
